{
  "name": "classifier.js",
  "version": "1.1.1",
  "description": ":robot: Natural language processing with Javascript",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc --declaration",
    "push:tags": "git push origin --tags",
    "publish": "npm run build && npm publish",
    "version:patch": "npm version patch -m 'chore: release version %s' && npm run push:tags",
    "version:minor": "npm version minor -m 'chore: release version %s' && npm run push:tags",
    "version:major": "npm version major -m 'chore: release version %s' && npm run push:tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NathanFirmo/natural.js.git"
  },
  "keywords": [
    "neural-network",
    "natural-language-processing",
    "artificial-intelligence",
    "machine-learning",
    "nodejs",
    "classifier",
    "ai"
  ],
  "author": "Nathan Firmo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NathanFirmo/classifier.js/issues"
  },
  "homepage": "https://github.com/NathanFirmo/classifier.js#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "husky": "^8.0.1",
    "ts-loader": "^9.3.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.2"
  }
}
